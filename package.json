{
  "name": "rondel",
  "version": "1.0.6",
  "description": "Javascript library that makes use of the Proxy API to control objects through the use of handlers.",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack",
    "test": "cross-env NODE_ENV=dev jest",
    "test:prod": "cross-env NODE_ENV=prod jest --coverage",
    "test:watch": "cross-env NODE_ENV=dev jest --watchAll",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alfdocimo/rondel"
  },
  "author": "Alfredo Narvaez Docimo <alfredonado@gmail.com> (https://github.com/alfdocimo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alfdocimo/rondel/issues"
  },
  "homepage": "https://github.com/alfdocimo/rondel",
  "keywords": [
    "library",
    "starter",
    "es6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.0",
    "camelcase": "^5.3.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "jest": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^1.18.2",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {},
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 65,
        "branches": 65,
        "functions": 65,
        "lines": 65
      }
    }
  },
  "jestSonar": {
    "env": {
      "prod": {
        "reportPath": "./reports/prod",
        "reportFile": "test-reporter.xml",
        "indent": 4
      },
      "dev": {
        "reportPath": "./reports/dev",
        "reportFile": "test-reporter.xml",
        "indent": 4
      }
    }
  }
}
